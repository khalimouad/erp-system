<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Account Types -->
        <record id="account_type_receivable" model="account.account.type">
            <field name="name">Receivable</field>
            <field name="type">receivable</field>
            <field name="internal_group">asset</field>
        </record>
        <record id="account_type_payable" model="account.account.type">
            <field name="name">Payable</field>
            <field name="type">payable</field>
            <field name="internal_group">liability</field>
        </record>
        <record id="account_type_liquidity" model="account.account.type">
            <field name="name">Liquidity</field>
            <field name="type">liquidity</field>
